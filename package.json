{
  "name": "graphiql-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "i18nexus pull && next dev",
    "start": "i18nexus pull && next start",
    "build": "tsc -b && i18nexus pull && next build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "next build && next start",
    "prettier": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "prettier:fix:locales": "prettier --write locales/**/*.json",
    "types-check": "tsc --noEmit",
    "format:fix": "npm run lint:fix && npm run prettier:fix",
    "validate:all": "npm run lint && npm run prettier && npm run types-check",
    "prepare": "husky",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@hookform/resolvers": "^3.9.0",
    "@nextui-org/react": "^2.4.6",
    "@reduxjs/toolkit": "^2.2.6",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@uiw/react-codemirror": "^4.23.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "firebase": "^10.13.0",
    "framer-motion": "^11.3.28",
    "graphql": "^16.9.0",
    "i18next": "^23.14.0",
    "i18next-resources-to-backend": "^1.2.1",
    "next": "14.2.5",
    "next-i18n-router": "^5.5.1",
    "notistack": "^3.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.52.2",
    "react-i18next": "^15.0.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "sass": "^1.77.8",
    "vite-tsconfig-paths": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18next": "^6.0.9",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-8d8bd53-20240905",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.4",
    "i18nexus-cli": "^3.5.0",
    "jsdom": "^25.0.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^2.0.5"
  }
}
